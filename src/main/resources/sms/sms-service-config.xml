<?xml version="1.0" encoding="UTF-8"?>
<application-config>

	<!-- 
		Filename of default file.
	 -->
    <parameter name="DataFile">
         <value>sms/sms.xml</value>
    </parameter>
    
    <!-- 
    	Filename of external file. If this exists the defaults file is not used.
     -->
	<parameter name="DataFileExternal">
         <value></value>
    </parameter>

	<!-- 
		Value in seconds between checking the files for updates.
	 -->
	<parameter name="UpdateFrequency">
         <value>3600</value>
    </parameter>

</application-config>