<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<application-config>

    <parameter name="Decision">
        <list>
            <value>/decision/decision-config.xml</value>
        </list>
    </parameter>

    <parameter name="DecisionJSPInclude">
        <value>/dialogs/framework/decision/decision_template.jsp</value>
    </parameter>
    
    <parameter name="uri">
        <value>.sedecision</value>
    </parameter>
    
    <parameter name="timeout">
        <value>10s</value>
    </parameter>
    
    <parameter name="DecisionView">
        <value>DecisionView</value>
    </parameter>
    
    <!-- description: VXML variable names within scope of the subdialog generated by the dynamic audio tag. The variable(s) are included in the subdialog namelist
         - language
         - library
         - version 
    -->
    <parameter name="CachePropertyKeys">
        <list>
            <value>language</value>
            <value>library</value>
            <value>version</value>
        </list>
    </parameter>
    
</application-config>
