		</parameter>
	
		<parameter name="exittags">
			<map>
				<parameter name="className">
					<value>com.nuance.ps.telefonica.exittags.ExitTagServiceManagerImpl</value>
				</parameter>
				<parameter name="resourceLocation">
					<value>context</value>
				</parameter>
				<parameter name="serviceFileName">
					<value>/exittags-service-config.xml</value>
				</parameter>
			</map>
		</parameter>
